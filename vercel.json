{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_SITE_URL": "https://digiclickai.com",
    "NEXT_PUBLIC_APP_URL": "https://digiclickai.com",
    "NEXT_PUBLIC_API_URL": "https://digiclickai.com/api"
  },
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ]
}
